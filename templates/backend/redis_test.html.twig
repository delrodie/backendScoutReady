        <div class="content">
            <div class="row">
                <div class="col-md-10 col-lg-8 col-xl-6 offset-md-1 offset-lg-2 offset-xl-3">
                    <h4 class="content-label">Test redis</h4>
                    <hr>
                    <div class="mt-3 mb-5 text-end">
                        <a href="{{ absolute_url(path('app_config_redis_test')) }}" class="btn btn-blue ajouter">Test fonctionnel</a>
                    </div>
                    <div class="card">
